#include <stdio.h>

/**
* main - causes an infinite loop
* Return: 0
*/

int main(void)
{
	int i;

	printf("Infinite loop incoming :(\n");
/** printf - prints what's about to happen */
	i = 0;
/**
*while (i < 10) {
* putchar(i);
* }
* printf prints the t
*/
	printf("Infinite loop avoided! \\o/\n");

	return (0);
}
